{
  "name": "dex-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "env VITE_API_URL=http://localhost:8012 VITE_CARDANO_NETWORK=preprod VITE_SENTRY_ENVIRONMENT=development-preprod vite --force",
    "serve": "serve --no-clipboard --single --no-port-switching dist",
    "build": "env NODE_OPTIONS=--max-old-space-size=8192 vite build && cp serve.json dist/",
    "preview": "vite preview",
    "eslint:check": "eslint ./src --ext .jsx,.js,.ts,.tsx --ignore-path ./.gitignore",
    "eslint:fix": "yarn eslint:check --quiet --fix",
    "prettier:check": "prettier --check --loglevel warn --ignore-path ./.gitignore .",
    "prettier:fix": "yarn prettier:check --write",
    "lint:check": "yarn eslint:check && yarn prettier:check ",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix ",
    "type-check": "tsc --noEmit",
    "deploy:mainnet": "env TARGET=mainnet bash ./deploy.sh",
    "deploy:preprod": "env TARGET=preprod bash ./deploy.sh",
    "generate": "npx @rtk-query/codegen-openapi openapi-config.ts",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier:fix",
      "yarn eslint:fix"
    ]
  },
  "dependencies": {
