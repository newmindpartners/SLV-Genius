{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "params": {
      "type": "object",
      "properties": {
        "roundId": {
          "type": "string"
        }
      },
      "required": ["roundId"]
    },
    "body": {
      "type": "object",
      "properties": {
        "file": {
          "type": "object",
          "properties": {
            "data": {
              "type": "string",
              "format": "binary"
            },
            "name": {
              "type": "string"
            }
          },
          "required": ["data", "name"]
        }
      },
      "required": ["file"]
    }
  }
}
