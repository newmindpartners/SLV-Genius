{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "count": {
            "type": "number"
        },
        "next": {
            "type": "string"
        },
        "previous": {
            "type": "string"
        },
        "results": {
            "items": {
                "allOf": [
                    {
                        "properties": {
                            "stakingProjectId": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "stakingProjectId"
                        ],
                        "type": "object"
                    },
                    {
                        "properties": {
                            "imageLogoUrl": {
                                "type": "string"
                            },
                            "lockDurationOptions": {
                                "items": {
                                    "enum": [
                                        "FLEX",
                                        "MONTHS_1",
                                        "MONTHS_12",
                                        "MONTHS_3",
                                        "MONTHS_6",
                                        "MONTHS_9"
                                    ],
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            "stakingAsset": {
                                "allOf": [
                                    {
                                        "properties": {
                                            "assetId": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "assetId"
                                        ],
                                        "type": "object"
                                    },
                                    {
                                        "properties": {
                                            "assetId": {
                                                "type": "string"
                                            },
                                            "assetName": {
                                                "type": "string"
                                            },
                                            "decimalPrecision": {
                                                "type": "number"
                                            },
                                            "enabled": {
                                                "type": "boolean"
                                            },
                                            "iconUrl": {
                                                "type": "string"
                                            },
                                            "longName": {
                                                "type": "string"
                                            },
                                            "policyId": {
                                                "type": "string"
                                            },
                                            "referenceAssetAmount": {
                                                "type": "string"
                                            },
                                            "referenceAssetId": {
                                                "type": "string"
                                            },
                                            "shortName": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "assetId",
                                            "assetName",
                                            "decimalPrecision",
                                            "iconUrl",
                                            "longName",
                                            "policyId",
                                            "shortName"
                                        ],
                                        "type": "object"
                                    }
                                ]
                            }
                        },
                        "required": [
                            "imageLogoUrl",
                            "lockDurationOptions",
                            "stakingAsset"
                        ],
                        "type": "object"
                    }
                ]
            },
            "type": "array"
        }
    },
    "required": [
        "count",
        "results"
    ],
    "type": "object"
}

