{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "allOf": [
    {
      "properties": {
        "walletAddress": {"type": "string"}
      },
      "required": ["walletAddress"],
      "type": "object"
    },
    {
      "properties": {
        "collateralUtxo": {"type": "array", "items": {"type": "string"}},
        "walletRewardAddresses": {"type": "array", "items": {"type": "string"}},
        "walletUnusedAddresses": {"type": "array", "items": {"type": "string"}},
        "walletUsedAddresses": {"type": "array", "items": {"type": "string"}}
      },
      "required": [
