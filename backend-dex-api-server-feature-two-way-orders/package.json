{
  "name": "dex-api-server",
  "version": "1.0.0",
  "private": "true",
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "test": "jest",
    "fix": "gts fix",
    "lint": "gts lint",
    "type-check": "tsc --noEmit",
    "clean": "gts clean",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "start": "node -r source-map-support/register dist/src/server.js",
    "cli": "node dist/src/cli/server/index.js",
    "cli:server": "DISABLE_REDIS=true ts-node-dev --files -r tsconfig-paths/register src/cli/server/index.ts",
    "cli:tools": "ts-node-dev --files -r tsconfig-paths/register src/cli/tools/index.ts",
    "seed": "ts-node-dev --files -r tsconfig-paths/register src/seed/scripts/index.ts",
    "seed:reset": "prisma migrate reset --skip-seed",
    "queue-transaction": "node dist/src/queueProcessors/transactionSubmit/index.js",
    "queue-transaction:dev": "ts-node-dev --files -r tsconfig-paths/register src/queueProcessors/transactionSubmit/index.ts",
    "db-schema": "node dist/src/seed/misc/archiveSchema.js",
    "db-schema:dev": "ts-node-dev --files -r tsconfig-paths/register src/seed/misc/archiveSchema.ts",
    "start:dev": "ts-node-dev --exit-child -r tsconfig-paths/register src/server.ts | pino-pretty",
    "start:debug": "ts-node-dev --exit-child --transpile-only --inspect=0.0.0.0:9229 -r tsconfig-paths/register src/server.ts | pino-pretty",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev",
    "generate:prisma": "prisma generate",
    "generate:open-api:public": "oazapfts specification/public/open-api/future.yml src/domain/models/public/index.ts",
    "generate:open-api:sumsub": "openapi-generator-cli generate -i specification/client/open-api/sumsub.yml -o src/implementation/client/sumsub -g typescript-axios",
    "generate:open-api": "yarn generate:open-api:public && yarn generate:open-api:sumsub",
    "generate:serializer-schemas": "./generate-serializer-schemas.sh",
    "deploy:mainnet": "env TARGET=mainnet bash ./deploy.sh",
    "deploy:preprod": "env TARGET=preprod bash ./deploy.sh"
  },
  "dependencies": {
    "@aws-sdk/util-hex-encoding": "^3.109.0",
    "@dcspark/cardano-multiplatform-lib-nodejs": "6.0.1",
    "@fastify/basic-auth": "^4.0.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^10.0.1",
    "@prisma/client": "4.16.0",
    "@sentry/node": "7.12.1",
    "async-lock": "^1.4.0",
    "axios": "^1.12.0",
    "bech32": "^2.0.0",
    "big.js": "^6.2.1",
    "blake2b": "^2.1.4",
    "bullmq": "3.2.0",
    "commander": "^9.4.1",
    "csv-parse": "^5.3.3",
    "d3": "v7.9.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "fastify": "^4.8.1",
    "fastify-cors": "^6.1.0",
    "fastify-plugin": "^4.0.0",
    "fp-ts": "^2.12.2",
    "fraction.js": "^4.3.7",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "pino": "^8.6.1",
    "prisma": "4.16.0",
    "prisma-redis-middleware": "^4.8.0",
    "reflect-metadata": "^0.1.13",
    "sinclair": "^0.2.0",
    "ssl-root-cas": "^1.3.1",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@blockfrost/blockfrost-js": "^5.1.0",
    "@blockfrost/openapi": "^0.1.59",
    "@jest/types": "^29.0.2",
    "@mermaid-js/mermaid-cli": "^10.1.0",
    "@openapitools/openapi-generator-cli": "^2.13.7",
    "@prisma/generator-helper": "^4.14.0",
    "@types/async-lock": "^1.3.0",
    "@types/big.js": "^6.1.6",
    "@types/blake2b": "^2.1.0",
    "@types/d3": "v7.4.3",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "csv-writer": "^1.6.0",
    "eslint": "8.22.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^4.0.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.1.2",
    "jest-ts-auto-mock": "^2.1.0",
    "oazapfts": "^3.6.0",
    "openapi-typescript-codegen": "^0.23.0",
    "pino-pretty": "^9.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-prisma": "4.15.0",
    "prisma-erd-generator": "^1.5.4",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "^1.3.8",
    "tsconfig-paths": "^3.10.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-json-schema": "^0.63.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
